{
  "name": "eslint-config-react-typescript",
  "version": "1.0.5",
  "description": "One config to rule them all",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Yulong Ruan <ruanyu1@gmail.com>",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^3.1.0",
    "eslint-plugin-jsdoc": "^27.0.5",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4"
  },
  "devDependencies": {
    "@types/eslint": "^6.8.1",
    "@types/node": "^14.0.12",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "eslint": "^7.2.0",
    "prettier": "^2.0.5",
    "tsconfigs": "^5.0.0",
    "typescript": "^3.9.5",
    "standard-version": "^7.1.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=3.1.0",
    "eslint": ">=7.2.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "useTabs": false,
    "bracketSpacing": false,
    "arrowParens": "avoid",
    "parser": "typescript",
    "endOfLine": "lf",
    "jsxBracketSameLine": false,
    "proseWrap": "preserve"
  },
  "license": "MIT"
}
